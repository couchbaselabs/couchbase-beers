<div class="vproduct" data-last-updated="{{updated}}">
  <h1 class="fn">{{name}}</h1>
  <h4 class="brand">by <a href="{{brewery_url}}">{{brewery}}</a></h4>
  <ul class="categories">
    <li>{{category}}</li>
    <li>{{style}}</li>
  </ul>
  <div class="description">{{description}}</div>
  <ul class="attribs">
    {{#abv}}<li class="abv"><abbr title="alcohol by volume">ABV</abbr>: {{abv}}</li>{{/abv}}
  </ul>
</div>